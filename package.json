{
	"name": "speedtest",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"ts": "tsc --noEmit --incremental false",
		"format": "prettier --loglevel silent --write .",
		"format:check": "prettier --check ."
	},
	"dependencies": {
		"next": "13.3.1",
		"postcss": "8.4.23",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tailwind-merge": "^1.12.0"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^13.0.0",
		"@pnxdxt/eslint-config": "latest",
		"@pnxdxt/prettier-config": "latest",
		"@types/node": "18.16.3",
		"@types/react": "18.2.0",
		"@types/react-dom": "18.2.1",
		"autoprefixer": "10.4.14",
		"eslint": "8.39.0",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"tailwindcss": "3.3.2",
		"typescript": "5.0.4"
	},
	"nextBundleAnalysis": {
		"budget": null,
		"budgetPercentIncreaseRed": 20,
		"showDetails": true,
		"minimumChangeThreshold": 300
	},
	"packageManager": "yarn@3.5.0",
	"author": {
		"name": "pnxdxt",
		"email": "pnxdxt@gmail.com",
		"url": "https://pnxdxt.com"
	}
}
